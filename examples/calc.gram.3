EXTEND
  e_top: [ [ x = e -> x ] ] ;

  e_star: [ [ x = e LEVEL "*" -> x ] ] ;

  e: AFTER "**"
    [ NONA [ x = INT -> int_of_string x
      | "("; x = e; ")" -> x ] ]
  ;

  e: BEFORE "*"
    [ LEFTA [ x1 = e; "+"; y = e -> x1 + y
      | x2 = e; "-"; y = e -> x2 - y ]
    ]
  ;

  e: AFTER "*"
    [ "**" RIGHTA [ x = e; "**"; y = e -> pow x  y ]
    ]
  ;

  e:
    [ "*" LEFTA [ x = e; "*"; y = e -> x * y
      | x = e; "/"; y = e -> x / y ] ]
  ;

END;
